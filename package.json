    "react-virtuoso": "^4.12.6"
  },
  "loki": {
    "looks-same": {
      "ignoreCaret": true
    },
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "overrides": {
    "storybook": "$storybook"
  }
}
